<Window x:Class="ESL_WPF.ImageDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ESL_WPF"
        mc:Ignorable="d"
        Title="ImageDialog" Height="228" Width="617">
    <Grid>
        <Label x:Name="label" Content="回報人員" HorizontalAlignment="Left" Margin="100,39,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy" Content="問題發布時間:" HorizontalAlignment="Left" Margin="100,89,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.095,-2.54"/>
        <Label x:Name="ErrorTime" Content="回報人員" HorizontalAlignment="Left" Margin="232,89,0,0" VerticalAlignment="Top"/>
        <Label x:Name="ReturnWorkerLabel" Content="回報人員" HorizontalAlignment="Left" Margin="232,39,0,0" VerticalAlignment="Top"/>
        <Button x:Name="OKButton" Content="OK" HorizontalAlignment="Left" Margin="320,159,0,0" VerticalAlignment="Top" Width="75" Click="OKButton_Click"/>
        <Button x:Name="SolvedButton" Content="問題排除" HorizontalAlignment="Left" Margin="482,159,0,0" VerticalAlignment="Top" Width="76" Click="SolvedButton_Click"/>

    </Grid>
</Window>
